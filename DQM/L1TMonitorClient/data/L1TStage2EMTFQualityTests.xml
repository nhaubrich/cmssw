<TESTSCONFIGURATION>

    <QTEST name="DeadChannels_EMTF_2D">
        <TYPE>DeadChannel</TYPE>
        <PARAM name="error">0.80</PARAM>
        <PARAM name="warning">0.50</PARAM>
        <PARAM name="threshold">0</PARAM>
    </QTEST>

    <QTEST name="HotChannels_EMTF_2D">
        <TYPE>NoisyChannel</TYPE>
        <PARAM name="error">0.80</PARAM>
        <PARAM name="warning">0.70</PARAM>
        <PARAM name="tolerance">10</PARAM>
        <PARAM name="neighbours">3</PARAM>
    </QTEST>

    <LINK name="*emtfChamberOccupancy">
        <TestName activate="true">DeadChannels_EMTF_2D</TestName>
        <TestName activate="true">HotChannels_EMTF_2D</TestName>
    </LINK>

    <LINK name="*emtfTrackOccupancy">
        <TestName activate="true">DeadChannels_EMTF_2D</TestName>
        <TestName activate="true">HotChannels_EMTF_2D</TestName>
    </LINK>


</TESTSCONFIGURATION>

